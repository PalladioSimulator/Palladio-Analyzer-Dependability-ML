<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.dependability.ml.feature"
      label="Sensitivity Analysis for Machine Learning Models"
      version="1.0.0.qualifier"
      plugin="org.palladiosimulator.branding"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="http://palladiosimulator.org">
      The plug-in allows ML models to be loaded and provides sensitivity analyses that can be applied to the machine learning model.
   </description>
   
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="com.google.guava" version="27.1.0" match="greaterOrEqual"/>
      <import plugin="com.google.gson" version="2.8.2" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.envdyn.model" version="0.1.0" match="greaterOrEqual"/>
      <import plugin="tools.mdsd.probdist.api" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="tools.mdsd.probdist.model.basic" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.dependability.ml" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.envdyn.api" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.13.700" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.debug.ui" version="3.14.400" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.dependability.ml.sensitivity" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.workflow.launchconfig" version="4.2.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.palladiosimulator.dependability.ml"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.dependability.ml.sensitivity"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.dependability.ml.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
